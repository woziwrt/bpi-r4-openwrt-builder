-				mediatek,mt753x)
-                    if [ "$status" = "okay" ]; then
-                        dts_is_gsw=1
-                    fi
-                    ;;
-            esac
-        fi
-    done
-
-	# Sanity check for both dsa and gsw is true, always configure it to dsa
-    if [ "$dts_is_gsw" -gt 0 ] && [ "$dts_is_dsa" -eq 0 ]; then
